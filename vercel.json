{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/cotacao",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/api.bcb",
      "dest": "./lib/api.bcb.js",
      "methods": [
        "GET"
      ]
    }
  ]
}